@prefix rdf:   		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 		<http://www.w3.org/2000/01/rdf-schema#"> .
@prefix owl:   		<http://www.w3.org/2002/07/owl#> .
@prefix lapis:   	<http://localhost:8080/CognitiveApp/files/ontologies/lapis#> .
@prefix sp:		<http://surgipedia.sfb125.de/wiki/Special:URIResolver/> .
@prefix xsd:	 	<http://www.w3.org/2001/XMLSchema#> .
@prefix dc:		<http://dublincore.org/documents/2012/06/14/dcmi-terms/?v=elements#> .

###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#hasAbstract
lapis:hasAbstract
		rdf:type 	rdf:Property;
		rdfs:comment 	"Abstract of Service Functionality";
		rdfs:domain 	sp:Category:Coginitive_App;
		rdfs:range 	xsd:string.

###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#hasInputDescription
lapis:hasInputDescription
		rdf:type 	rdf:Property;
		rdfs:comment 	"Description of Input Parameters";
		rdfs:domain 	sp:Category:Coginitive_App;
		rdfs:range 	xsd:string.

###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#hasOutputDescription
lapis:hasOutputDescription
		rdf:type 	rdf:Property;
		rdfs:comment 	"Description of Output";
		rdfs:domain 	sp:Category:Coginitive_App;
		rdfs:range 	xsd:string.

###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#hasExampleRequest
lapis:hasExampleRequest
		rdf:type 	rdf:Property;
		rdfs:comment 	"Example Request for Invocation";
		rdfs:domain 	sp:Category:Coginitive_App;
		rdfs:range 	xsd:string.
		
###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#hasExampleResponse
lapis:hasExampleResponse
		rdf:type 	rdf:Property;
		rdfs:comment 	"Example Response";
		rdfs:domain 	sp:Category:Coginitive_App;
		rdfs:range 	xsd:string.

###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#Request
lapis:Request
		rdf:type	rdfs:Class;
		rdfs:comment 	"The class containing all requests.".
					
	
