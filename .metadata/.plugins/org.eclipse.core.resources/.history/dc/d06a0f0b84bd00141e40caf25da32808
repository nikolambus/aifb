###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#boolean
lapis:boolean
		rdf:type	rdfs:Datatype;
		rdfs:comment "CTK Input-Type: Boolean" ;
		owl:equivalentClass	xsd:boolean.
		
###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#integer
lapis:integer
		rdf:type	rdfs:Datatype;
		rdfs:comment "CTK Input-Type: Integer" ;
		owl:equivalentClass xsd:integer.
		
