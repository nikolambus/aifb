###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#integer-vector
lapis:integer-vector
		rdf:type	rdfs:Datatype;
		rdfs:comment "CTK Input-Type: Integer-vector" ;
		owl:onDatatype xsd:string ;
		owl:withRestrictions (
		[
			xsd:pattern "[\-+]?[0-9]+[,]*"
		]
	) .
	
###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#float-vector
lapis:float-vector
		rdf:type	rdfs:Datatype;
		rdfs:comment "CTK Input-Type: Float-vector" ;
		owl:onDatatype xsd:string ;
		owl:withRestrictions (
		[
			xsd:pattern "[\-+]?[0-9][\.]?[,]*"
		]
	) .	

###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#double-vector
lapis:double-vector
		rdf:type	rdfs:Datatype;
		rdfs:comment "CTK Input-Type: Double-vector" ;
		owl:onDatatype xsd:string ;
		owl:withRestrictions (
		[
			xsd:pattern "[\-+]?[0-9][\.]?[,]*"
		]
	) .	
	
###  http://localhost:8080/CognitiveApp/files/ontologies/lapis#string-vector
lapis:string-vector
		rdf:type	rdfs:Datatype;
		rdfs:comment "CTK Input-Type: String-vector" ;
		owl:onDatatype xsd:string ;
		owl:withRestrictions (
		[
			xsd:pattern "[a-zA-Z0-9][,]*"
		]
	) .	
		
