    @prefix xnatpatient: <http://aifb-ls3-vm2.aifb.kit.edu:8080/xnatwrapper/id/project/Liver_Factors/Patient#> .
    
    @prefix surgi: <http://surgipedia.sfb125.de/wiki/Special:URIResolver/> .
	@prefix surgiProp: <http://surgipedia.sfb125.de/wiki/Special:URIResolver/Property-3A> .
	@prefix surgiCat: <http://surgipedia.sfb125.de/wiki/Special:URIResolver/Category-3A> .
    
    @prefix log: <http://www.w3.org/2000/10/swap/log#>.
    @prefix math: <http://www.w3.org/2000/10/swap/math#>.
    @prefix time: <http://www.w3.org/2000/10/swap/time#>.
    @prefix string: <http://www.w3.org/2000/10/swap/string#>.
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#yes-yes-path of decision tree

{
 ?o1 a surgiCat:Observation ;
 	 surgiProp:hasPatient ?p ;
     surgiProp:hasFactor surgi:NumberOfTumors ; 
     surgiProp:hasObsValue ?not . 
 ?not math:notGreaterThan 1 .  
 
 ?o2 a surgiCat:Observation ; 
	 surgiProp:hasPatient ?p ;
     surgiProp:hasFactor surgi:MaxTumorSize ; 
     surgiProp:hasObsValue ?mts . 
 ?mts math:lessThan 6.5  
} => {
	_:SFbnode a surgiCat:Observation .
	_:SFbnode surgiProp:hasPatient ?p .
	_:SFbnode surgiProp:hasFactor surgi:SF_criteria .
	_:SFbnode surgiProp:hasObsValue "SF_criteria_fulfilled" .
} .
	
#no-yes-yes-yes-path of decision tree
{
 ?o1 a surgiCat:Observation ; 
 	 surgiProp:hasPatient ?p ;
  	 surgiProp:hasFactor surgi:NumberOfTumors ; 
 	 surgiProp:hasObsValue ?not . 
 ?not math:greaterThan 1 ;  
      math:lessThan 4 .
 
 ?o2 a surgiCat:Observation ; 
  	 surgiProp:hasPatient ?p ;
     surgiProp:hasFactor surgi:MaxTumorSize ; 
     surgiProp:hasObsValue ?mts . 
 ?mts math:notGreaterThan 4.5 .
 
 ?o3 a surgiCat:Observation ; 
  	 surgiProp:hasPatient ?p ;
   	 surgiProp:hasFactor surgi:TotalTumorSize ; 
   	 surgiProp:hasObsValue ?tts . 
 ?tts math:lessThan 8    
} => {
	_:SFbnode a surgiCat:Observation .
	_:SFbnode surgiProp:hasPatient ?p .
	_:SFbnode surgiProp:hasFactor surgi:SF_criteria .
	_:SFbnode surgiProp:hasObsValue "SF_criteria_fulfilled" .
} .	

