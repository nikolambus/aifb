#11
{ 
 surgi:scoresHelpNode surgiProp:hasPatient ?p .
 surgi:scoresHelpNode surgiProp:hasScorePosNode ?posNode .
 surgi:scoresHelpNode surgiProp:hasScoreInterval ?interval .
 surgi:scoresHelpNode surgiProp:hasScoreNumTum ?num .
 surgi:scoresHelpNode surgiProp:hasScoreCEA ?cea .
 surgi:scoresHelpNode surgiProp:hasScoreMaxTumorSize ?msize .
 (?posNode ?interval ?num ?cea ?msize) math:sum ?s .
 ?s math:equalTo 5
} => {
	_:FongObs a surgiCat:Observation .
	_:FongObs surgiProp:hasFactor surgi:Fong .
	_:FongObs surgiProp:hasPatient ?p .
	_:FongObs surgiProp:hasObsValue "Prognostic survival of 15 months" .
	} .
	
