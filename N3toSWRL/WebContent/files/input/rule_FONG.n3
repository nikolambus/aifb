#1    
{ 
 ?o1 a surgiCat:Observation . 
 ?o1 surgiProp:hasFactor surgi:PositiveLymphNode .
 ?o1 surgiProp:hasObsValue "no" .
 ?o1 surgiProp:hasPatient ?p
} => {
	surgi:scoresHelpNode a log:Chaff .
	surgi:scoresHelpNode surgiProp:hasScorePosNode 0 ;
						surgiProp:hasPatient ?p .
	} .

